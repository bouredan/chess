<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="800.0" prefWidth="800.0" stylesheets="@board.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="cz.cvut.fel.bouredan.chess.gui.BoardController">
    <top>
        <Button fx:id="startButton">Start</Button>
    </top>
    <center>
        <GridPane fx:id="board" alignment="CENTER" prefHeight="500.0" prefWidth="500.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" percentHeight="12.5" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
    </center>
</BorderPane>
