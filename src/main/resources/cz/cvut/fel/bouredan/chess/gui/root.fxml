<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import cz.cvut.fel.bouredan.chess.gui.game.BoardView?>
<BorderPane fx:id="rootBorderPane" xmlns:fx="http://javafx.com/fxml" prefHeight="800.0" prefWidth="1100.0"
            fx:controller="cz.cvut.fel.bouredan.chess.gui.ApplicationController" stylesheets="@board.css">
    <top>
        <Label>Chess</Label>
        <Button fx:id="startButton" onAction="#startNewGame">Start game</Button>
    </top>
    <center>
        <BoardView fx:id="boardView"/>
    </center>
</BorderPane>